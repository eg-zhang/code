{
    "name": "sprite坐标",
    "version": "1.0",
    "manifest_version":2,
    "description": "显示当前鼠标在sprite上的坐标",
    "background": {
        "scripts": ["js/background.js"],
        "persistent":false
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": ["tabs","cookies","http://*/*"]
}
